$lineAdded = rgb(0,255,0)
$lineDeleted = rgb(255,0,0)

body
    margin 20px
    font-size 0.8em

body,
code
    font-family Monaco, monospace, sans-serif

pre
    background-color rgb(200,200,200)

code#fileContent
    font-size 0.9em
    
span.lineAdded
    background-color: $lineAdded

span.lineDeleted
    background-color: $lineDeleted
    